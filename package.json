    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/react-test-renderer": "^16.9.1",
    "eslint": "^6.8.0",
    "husky": "6.0.0",
    "jest-dom": "^4.0.0",
    "lint-staged": "^13.1.0",
    "ts-jest": "^29.1.0"
  },
  "eslintIgnore": [
    "node_modules/*",
    "target/*"
